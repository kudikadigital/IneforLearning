    function adicionarCurso() {
      const nomeCurso=document.getElementById('nomeCurso').value;
      const descricaoCurso1=document.getElementById('descricaoCurso1').value;
      const idCategoria=document.getElementById('idCategoria').value;
      const descricaoCurso=document.getElementById('descricaoCurso').value;
      const precoCurso=document.getElementById('precoCurso').value;
      const cargaHoraria=document.getElementById('cargaHoraria').value;
      const aprendizadoCurso=document.getElementById('aprendizadoCurso').value;
      const idProf="<%= professor.idProf%>"
      const imgCurso=document.getElementById('avatarNewProjectUploader').files[0]
      const Dados= new FormData();
      Dados.append('nomeCurso',nomeCurso)
      Dados.append('descricaoCurso1',descricaoCurso1)
      Dados.append('idCategoria',idCategoria)
      Dados.append('descricaoCurso',descricaoCurso)
      Dados.append('precoCurso',precoCurso)
      Dados.append('idProf',idProf)
      Dados.append('cargaHoraria',cargaHoraria)
      Dados.append('aprendizadoCurso',aprendizadoCurso)
      Dados.append('image',imgCurso)

      const options = {
        method: 'POST',
        body: Dados,
      };

      fetch('http://localhost:1200/novoCurso', options).then(data => {
        return data.json();
      }).then(data => {
        if(data.error){
          document.getElementById('resultadoTitulo').innerHTML ='Ocorreu um erro!';
          document.getElementById('resultadoCorpo').innerHTML =data.error; 
          document.getElementById('resultadoCorpo').setAttribute('class', 'text-danger')
          document.getElementById('img').setAttribute('src', '/dashboard/assets/svg/illustrations/oc-error.svg')
        }else{
          document.getElementById('resultadoTitulo').innerHTML ='Sucesso!';
          document.getElementById('resultadoCorpo').innerHTML =data.certo;
          document.getElementById('resultadoCorpo').setAttribute('class', 'text-success')
          document.getElementById('img').setAttribute('src', '/dashboard/assets/svg/illustrations/oc-hi-five.svg')
          window.location="/cursoFormador";
        }
      })
      .catch(e => {
        console.log(e);
      });
    }
  </script>


